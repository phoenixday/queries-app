            <xs:element name="datumOd" type="xs:date" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Datum, od kdy je role osobě přisuzována.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="datumDo" type="xs:date" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Datum, do kdy je role osobě přisuzována.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="poznamka" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Poznámka k přiřazené roli osobě.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Role" type="Role" minOccurs="1" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Sbirka">
        <xs:annotation>
            <xs:documentation>Sbírka, kterou má ve správě správce sbírky, a zároveň 1. úroveň systematického členění pro zatřídění sbírkového předmětu. Další úrovně jsou: podsbírka, fond, skupina, podskupina1 až 3.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="id" type="xs:ID" minOccurs="1" maxOccurs="1"/>
            <xs:element name="cisloEvidInt" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Interní evidenční číslo. Interní číslo sbírky, které si zvolí správce sbírky jako číslo provizorní, než mu ministerstvo kultury přidělí oficiální evidenční číslo sbírky (číslo pro CES). </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="cisloEvidCES" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Oficiální evidenční číslo sbírky. Evidenční číslo sbírky přiděluje ministerstvo kultury, pod tímto číslem je sbírka zaevidována do CES.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="nazev" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Název sbírky. Např. Sbírka Muze hlavního města Prahy.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="popisPublic" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Popis sbírky pro publikaci.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="poznamka" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Poznámka.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="spravceSbirky" type="OaiId" minOccurs="1" maxOccurs="1"/>
            <xs:element name="vlastnik" type="OaiId" minOccurs="0" maxOccurs="1"/>
            <xs:element name="typVlastnika" type="TypSpravceVlastnika" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Skupina">
        <xs:annotation>
            <xs:documentation>Skupina - 4. úroveň systematického členění pro zatřídění sbírkového předmětu. Další úrovně jsou: podksupina1, podskupina2 a podskupina3.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="id" type="xs:ID" minOccurs="1" maxOccurs="1"/>
            <xs:element name="kod" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Kód systématické skupiny - kód skupiny.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="nazev" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Název systématické skupiny - název skupiny.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="poradi" type="xs:int" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Pořadí pro zobrazení na webovém portále.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="popisPublic" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Popis určený pro webový portál. </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="poznamka" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Poznámka.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="fond" type="OaiId" minOccurs="1" maxOccurs="1"/>
            <xs:element name="sbirka" type="OaiId" minOccurs="1" maxOccurs="1"/>
            <xs:element name="podsbirka" type="OaiId" minOccurs="1" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="SpravceSbirky">
        <xs:annotation>
            <xs:documentation>Správce sbírky muzejní povahy.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="id" type="xs:ID" minOccurs="1" maxOccurs="1"/>
            <xs:element name="nazev" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Zkrácený název (nebo zkratka) správce sbírky. Např.: MMP</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="poznamka" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Poznámka. </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="popisPublic" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Popis paměťové instituce - správce sbírky - pro publikaci.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="typ" type="TypSpravceVlastnika" minOccurs="0" maxOccurs="1"/>
            <xs:element name="subjekt" type="OaiId" minOccurs="1" maxOccurs="1"/>
            <xs:element name="nadrizeny" type="OaiId" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Subjekt">
        <xs:annotation>
            <xs:documentation>Subjekt (actor) - fyzická nebo právnická osoba (osoba/instituce) související se sbírkovým předmětem, vystupující v něaké roli (autor, grafik, fotograf, ... ). </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="id" type="xs:ID" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Identifikátor záznamu.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="kod" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Jednoznačný kód subjektu, např. zkratka jména osoby nebo názvu instituce.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="nazev" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Název, který právnickou osobu identifikuje ve státním rejstříku.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ic" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Identifikační číslo z rejstříku, ve kterém je PO zapsaná.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="dic" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Daňové identifikační číslo daňového subjektu.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="jmenoPrvni" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>První jméno osoby.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="jmenoDruhe" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Druhé jméno osoby.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="prijmeni" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Příjmení osoby.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="rodnePrijmeni" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Rodné příjmení osoby.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="titulPredJmenem" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Titul před jménem osoby.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="titulZaJmenem" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Titul za jménem osoby.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="medailon" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Medailon - životopis osobnosti (např. pro tisk do katalogů) nebo popis instituce, spolku apod.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="studia" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Studia - obvykle se uvádí doba studia, škola, případně učitel.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="spolky" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Spolky - spolky (zejména umělecké), kterých osoba byla nebo je členem.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="datumOd" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Datování narození osoby nebo založení instituce v textové podobě.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="datumDo" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Datování úmrtí osoby nebo ukončení činnosti instituce v textové podobě.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="datumNarozeni" type="xs:date" minOccurs="0" maxOccurs="1"/>
            <xs:element name="mistoNarozeni" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Místo narození osoby.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="datumUmrti" type="xs:date" minOccurs="0" maxOccurs="1"/>
            <xs:element name="mistoUmrti" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Místo úmrtí osoby.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="pohlavi" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Pohlaví osoby.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="text1" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Textový údaj č. 1 pro volné použití zákazníkem.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="text2" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Textový údaj č. 2 pro volné použití zákazníkem.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="text3" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Textový údaj č. 3 pro volné použití zákazníkem.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="poznamka" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Poznámka, libovolný text.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="typ" type="TypSubjektu" minOccurs="1" maxOccurs="1"/>
            <xs:element name="Kontakt" type="Kontakt" minOccurs="1" maxOccurs="1"/>
            <xs:element name="Adresa" type="Adresa" minOccurs="1" maxOccurs="1"/>
            <xs:element name="RoleOsoby" type="RoleOsoby" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="DalsiJmenoOsoby" type="DalsiJmenoOsoby" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="KontextovyDokument" type="KontextovyDokument" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="spravceSbirky" type="OaiId" minOccurs="1" maxOccurs="1"/>
            <xs:element name="podsbirka" type="OaiId" minOccurs="0" maxOccurs="1"/>
            <xs:element name="sbirka" type="OaiId" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="TechnikaPublic">
        <xs:annotation>
            <xs:documentation>Technika - slovník veřejného repozitáře.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="id" type="xs:ID" minOccurs="1" maxOccurs="1"/>
            <xs:element name="nazev" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Název techniky.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="spravceSbirky" type="OaiId" minOccurs="0" maxOccurs="1"/>
            <xs:element name="nadrizeny" type="OaiId" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="TypJmena">
        <xs:annotation>
            <xs:documentation>Typ jména osoby. Význam kódů: - PS - pseudonym - SJ - skutečné jméno - DV - další varianta jména - XX - neurčeno</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="PS"/>
            <xs:enumeration value="SJ"/>
            <xs:enumeration value="DV"/>
            <xs:enumeration value="XX"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TypPredmetu">
        <xs:annotation>
            <xs:documentation>Typ sbírkového předmětu (typ katalogizační karty). Význam kódů: P, obecný sbírkový předmět PA, předmět v archeologické sbírce FO, předmět sbírky fotografií VU, předmět sbírky výtvarného umění NU, předmět numismatické sbírky MI, předmět sbírky militárií DI, předmět divadelní sbírky BI, předmět obecné biologocké sbírky BO, předmět botanické sbírky ZO, předmět zoologické sbírky EN, předmět entomologické sbírky GE, předmět geologické sbírky PL, předmět paleontologické sbírky HM, předmět historické sbírky (spec. MMP) AU, movitý nález archeologie (spec. AUSAV) F1, fotografie objektu (spec. KTF) BR, záznam o knize (spec. Book Report)</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="P"/>
            <xs:enumeration value="PA"/>
            <xs:enumeration value="FO"/>
            <xs:enumeration value="VU"/>
            <xs:enumeration value="NU"/>
            <xs:enumeration value="MI"/>
            <xs:enumeration value="DI"/>
            <xs:enumeration value="BI"/>
            <xs:enumeration value="BO"/>
            <xs:enumeration value="ZO"/>
            <xs:enumeration value="EN"/>
            <xs:enumeration value="GE"/>
            <xs:enumeration value="PL"/>
            <xs:enumeration value="HM"/>
            <xs:enumeration value="AU"/>
            <xs:enumeration value="F1"/>
            <xs:enumeration value="BR"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TypPrilohyKd">
        <xs:annotation>
            <xs:documentation>Typ přílohy kontextového dokumentu.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="IMAGE"/>
            <xs:enumeration value="VIDEO"/>
            <xs:enumeration value="AUDIO"/>
            <xs:enumeration value="URL"/>
            <xs:enumeration value="DOC"/>
            <xs:enumeration value="OBJECT3D"/>
            <xs:enumeration value="PAPER"/>
            <xs:enumeration value="UNKNOWN"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="TypSbirkyPublic">
        <xs:annotation>
            <xs:documentation>Typ sbírky - slovník veřejného repozitáře.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="id" type="xs:ID" minOccurs="1" maxOccurs="1"/>
            <xs:element name="nazev" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Název typu sbírky.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="spravceSbirky" type="OaiId" minOccurs="0" maxOccurs="1"/>
            <xs:element name="nadrizeny" type="OaiId" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="TypSpravceVlastnika">
        <xs:annotation>
            <xs:documentation>Typ správce sbírky nebo vlastníka sbírky. Význam kódů: - S - Stát - K - Kraj - O - Obec - C - Církevní právnická osoba - P - Jiná právnická osoba - F - Jiná osoba</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="S"/>
            <xs:enumeration value="K"/>
            <xs:enumeration value="O"/>
            <xs:enumeration value="C"/>
            <xs:enumeration value="P"/>
            <xs:enumeration value="F"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TypSubjektu">
        <xs:annotation>
            <xs:documentation>Typ subjektu (osoba, instituce, ...). Význam kódů: - PO, Právnická osoba - OO, Osoba obecná - OZ, Osoba zaměstnanec</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="PO"/>
            <xs:enumeration value="OO"/>
            <xs:enumeration value="OZ"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="OaiId">
        <xs:annotation>
            <xs:documentation>Komplexní typu OaiId obsahuje hodnotu OAI identifikátoru, který byl záznamu v repozitáři přidělen.</xs:documentation>
        </xs:annotation>
        <xs:sequence/>
        <xs:attribute name="setSpec" use="optional" type="xs:string">
            <xs:annotation>
                <xs:documentation>Specifikace datové sady kam přísluší odkaz přes identifikátor oai_id. </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="oaiId" use="optional" type="xs:string">
            <xs:annotation>
                <xs:documentation>OAI identifikátor - jednoznačný v datovém repozitáři.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="id" use="optional" type="xs:string">
            <xs:annotation>
                <xs:documentation>Interní (lokální) identifikátor záznamu. </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="label" use="optional" type="xs:string">
            <xs:annotation>
                <xs:documentation>Název nebo kód nebo hlavní označení záznamu identifikovaného přes oai_id a datové sady z atributu setSpec.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
</xs:schema>